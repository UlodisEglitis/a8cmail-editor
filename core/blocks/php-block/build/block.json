{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/php-block",
  "version": "0.1.0",
  "title": "Todo List",
  "category": "widgets",
  "icon": "smiley",
  "description": "Block used to inject PHP into the template.",
  "attributes": {
    "message": {
      "type": "string",
      "default": "echo 'Hello World!';"
    }
  },
  "example": {
    "attributes": {
      "message": "PHP Contents"
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "php-block"
}